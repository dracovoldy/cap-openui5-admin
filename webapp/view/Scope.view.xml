<mvc:View controllerName="cap.estimate.controller.Scope" displayBlock="true" height="100%" xmlns="sap.m" xmlns:core="sap.ui.core"
	xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:mvc="sap.ui.core.mvc">
	<Page id="page2" showHeader="false" class="sapUiSizeCompact detailBg">
		<content>
			<Panel width="auto" class="sapUiTinyMargin bgPanel ">
				<headerToolbar>
					<Toolbar height="2rem" class="dtlHead">
						<Title text="Scope" level="H2" class="detailHeader1"/>
						<ToolbarSpacer/>
					</Toolbar>
				</headerToolbar>
				<content>
					<VBox class="bgWhiteImd">
						<Carousel class="sapUiNoContentPadding" arrowsPlacement="PageIndicator" pageIndicatorPlacement="Bottom">
							<pages>
								<VBox class="sapUiTinyMarginBegin sapUiTinyMarginEnd">
									<f:SimpleForm editable="true" layout="ColumnLayout" columnsM="2" columnsL="3" columnsXL="4">
										<f:content>
											<core:Title text="Region - North America" level="H4"/>
											<Label text="Plants" class="sapUiTinyMarginEnd"/>
											<Select id="NA_Plants" selectedKey="0" items="{/ScopeRegionSizes}">
												<items>
													<core:Item text="{desc}" key="{key}"/>
												</items>
											</Select>
											<!--<core:Title/>-->
											<Label text="Offices" class="sapUiTinyMarginEnd"/>
											<Select id="NA_Off" selectedKey="0" items="{/ScopeRegionSizes}">
												<items>
													<core:Item text="{desc}" key="{key}"/>
												</items>
											</Select>
											<!--<core:Title/>-->
											<Label text="Distribution Centres" class="sapUiTinyMarginEnd"/>
											<Select id="NA_Dcs" selectedKey="0" items="{/ScopeRegionSizes}">
												<items>
													<core:Item text="{desc}" key="{key}"/>
												</items>
											</Select>
											<!--<core:Title/>-->
											<Label text="Sales Offices" class="sapUiTinyMarginEnd"/>
											<Select id="NA_Sos" selectedKey="0" items="{/ScopeRegionSizes}">
												<items>
													<core:Item text="{desc}" key="{key}"/>
												</items>
											</Select>
										</f:content>
									</f:SimpleForm>
								</VBox>
								<VBox class="sapUiTinyMarginBegin sapUiTinyMarginEnd">
									<f:SimpleForm editable="true" layout="ColumnLayout" columnsM="2" columnsL="3" columnsXL="4">
										<f:content>
											<core:Title text="Region - Latin America" level="H4"/>
											<Label text="Plants" class="sapUiTinyMarginEnd"/>
											<Select id="LATAM_Plants" selectedKey="0" items="{/ScopeRegionSizes}">
												<items>
													<core:Item text="{desc}" key="{key}"/>
												</items>
											</Select>
											<!--<core:Title/>-->
											<Label text="Offices" class="sapUiTinyMarginEnd"/>
											<Select id="LATAM_Off" selectedKey="0" items="{/ScopeRegionSizes}">
												<items>
													<core:Item text="{desc}" key="{key}"/>
												</items>
											</Select>
											<!--<core:Title/>-->
											<Label text="Distribution Centres" class="sapUiTinyMarginEnd"/>
											<Select id="LATAM_Dcs" selectedKey="0" items="{/ScopeRegionSizes}">
												<items>
													<core:Item text="{desc}" key="{key}"/>
												</items>
											</Select>
											<!--<core:Title/>-->
											<Label text="Sales Offices" class="sapUiTinyMarginEnd"/>
											<Select id="LATAM_Sos" selectedKey="0" items="{/ScopeRegionSizes}">
												<items>
													<core:Item text="{desc}" key="{key}"/>
												</items>
											</Select>
										</f:content>
									</f:SimpleForm>
								</VBox>
								<VBox class="sapUiTinyMarginBegin sapUiTinyMarginEnd">
									<f:SimpleForm editable="true" layout="ColumnLayout" columnsM="2" columnsL="3" columnsXL="4">
										<f:content>
											<core:Title text="Region - Europe" level="H4"/>
											<Label text="Plants" class="sapUiTinyMarginEnd"/>
											<Select id="EU_Plants" selectedKey="0" items="{/ScopeRegionSizes}">
												<items>
													<core:Item text="{desc}" key="{key}"/>
												</items>
											</Select>
											<!--<core:Title/>-->
											<Label text="Offices" class="sapUiTinyMarginEnd"/>
											<Select id="EU_Off" selectedKey="0" items="{/ScopeRegionSizes}">
												<items>
													<core:Item text="{desc}" key="{key}"/>
												</items>
											</Select>
											<!--<core:Title/>-->
											<Label text="Distribution Centres" class="sapUiTinyMarginEnd"/>
											<Select id="EU_Dcs" selectedKey="0" items="{/ScopeRegionSizes}">
												<items>
													<core:Item text="{desc}" key="{key}"/>
												</items>
											</Select>
											<!--<core:Title/>-->
											<Label text="Sales Offices" class="sapUiTinyMarginEnd"/>
											<Select id="EU_Sos" selectedKey="0" items="{/ScopeRegionSizes}">
												<items>
													<core:Item text="{desc}" key="{key}"/>
												</items>
											</Select>
										</f:content>
									</f:SimpleForm>
								</VBox>
								<VBox class="sapUiTinyMarginBegin sapUiTinyMarginEnd">
									<f:SimpleForm editable="true" layout="ColumnLayout" columnsM="2" columnsL="3" columnsXL="4">
										<f:content>
											<core:Title text="Region - Asia Pacific" level="H4"/>
											<Label text="Plants" class="sapUiTinyMarginEnd"/>
											<Select id="APAC_Plants" selectedKey="0" items="{/ScopeRegionSizes}">
												<items>
													<core:Item text="{desc}" key="{key}"/>
												</items>
											</Select>
											<!--<core:Title/>-->
											<Label text="Offices" class="sapUiTinyMarginEnd"/>
											<Select id="APAC_Off" selectedKey="0" items="{/ScopeRegionSizes}">
												<items>
													<core:Item text="{desc}" key="{key}"/>
												</items>
											</Select>
											<!--<core:Title/>-->
											<Label text="Distribution Centres" class="sapUiTinyMarginEnd"/>
											<Select id="APAC_Dcs" selectedKey="0" items="{/ScopeRegionSizes}">
												<items>
													<core:Item text="{desc}" key="{key}"/>
												</items>
											</Select>
											<!--<core:Title/>-->
											<Label text="Sales Offices" class="sapUiTinyMarginEnd"/>
											<Select id="APAC_Sos" selectedKey="0" items="{/ScopeRegionSizes}">
												<items>
													<core:Item text="{desc}" key="{key}"/>
												</items>
											</Select>
										</f:content>
									</f:SimpleForm>
								</VBox>
							</pages>
						</Carousel>
						<HBox width="100%" class="sapUiLargeMarginTop sapUiMediumMarginBegin scopeCheckbx ">
							<VBox alignItems="Start" width="50%" class="sapUiSmallMarginEnd">
								<Title text="In scope business process"/>
								<HBox id="bp_box" width="100%" alignItems="Center">
									<VBox alignItems="Start" width="50%">
										<CheckBox id="FIN_Check" text="Finance"/>
										<CheckBox id="PTP_Check" text="Procure to Pay"/>
										<CheckBox id="MTS_Check" text="Maintenance"/>
										<CheckBox id="OTC_Check" text="Order to Cash"/>
									</VBox>
									<VBox alignItems="Start" width="50%">
										<CheckBox id="DTS_Check" text="Manufacturing"/>
										<CheckBox id="OTH_Check" text="Others"/>
										<TextArea id="OTH_Value" value="" growing="true" maxLength="250" width="100%"/>
									</VBox>
								</HBox>
							</VBox>
							<VBox alignItems="Start" width="50%">
								<HBox width="100%" alignItems="Center">
									<VBox alignItems="Start" width="100%" height="3em">
										<Label text="Number of integration test scenarios" class="sapUiTinyMarginEnd sapUiTinyMarginTop"/>
									</VBox>
									<VBox alignItems="Start" width="100%" height="3em">
										<Select id="Scenarios1" selectedKey="0" items="{/ScopeITS}">
											<items>
												<core:Item text="{desc}" key="{key}"/>
											</items>
										</Select>
									</VBox>
								</HBox>
								<HBox width="100%" alignItems="Center">
									<VBox alignItems="Start" width="100%" height="3em">
										<Label text="Number of unit test scripts" class="sapUiTinyMarginEnd sapUiTinyMarginTop"/>
									</VBox>
									<VBox alignItems="Start" width="100%" height="3em">
										<Select id="Scenarios2" selectedKey="0" items="{/ScopeITS}">
											<items>
												<core:Item text="{desc}" key="{key}"/>
											</items>
										</Select>
									</VBox>
								</HBox>
								<HBox width="100%" alignItems="Center">
									<VBox alignItems="Start" width="100%" height="3em">
										<Label text="Number of users" class="sapUiTinyMarginEnd sapUiTinyMarginTop"/>
									</VBox>
									<VBox alignItems="Start" width="100%" height="3em">
										<Select id="Scenarios3" selectedKey="0" items="{/NosUser}">
											<items>
												<core:Item text="{desc}" key="{key}"/>
											</items>
										</Select>
									</VBox>
								</HBox>
							</VBox>
						</HBox>
						<f:SimpleForm id="ScopeForm3" editable="false" layout="ResponsiveGridLayout" labelSpanXL="8" labelSpanL="8" labelSpanM="8" labelSpanS="12"
							adjustLabelSpan="false" emptySpanXL="0" emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="2" columnsL="2" columnsM="2"
							singleContainerFullSize="false">
							<f:content>
								<core:Title text="Number of languages"/>
								<Label text="Default English Only for system, data and training specify # of additional languages, if required"/>
								<Select id="Lang_Select" selectedKey="1">
									<items>
										<core:Item text="0" key="1"/>
										<core:Item text="1" key="2"/>
										<core:Item text="2" key="3"/>
										<core:Item text="3" key="4"/>
										<core:Item text="4" key="5"/>
										<core:Item text="5+" key="6"/>
									</items>
								</Select>
							</f:content>
						</f:SimpleForm>
					</VBox>
				</content>
			</Panel>
			<Panel width="auto" class="sapUiResponsiveMargin noBorder boxMdl">
				<Button text="Previous" type="Emphasized" class="backBtn"/>
				<ToolbarSpacer/>
				<Button text="Next" type="Emphasized" class="nextBtn" press="nextPress"/>
			</Panel>
		</content>
		<footer>
			<Toolbar height="1.5em" class="custFooter">
				<ToolbarSpacer/>
				<Label text="The information contained in this document is proprietary. Copyright © 2019 Capgemini. All rights reserved." class="labelFont">
					<layoutData>
						<ToolbarLayoutData shrinkable="true" minWidth="200px"/>
					</layoutData>
				</Label>
				<ToolbarSpacer/>
				<!--<Button text="Next" type="Emphasized" press="nextPress"/>-->
			</Toolbar>
		</footer>
	</Page>
</mvc:View>
